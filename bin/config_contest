#!/bin/bash

## Exit codes
##	1: can
ETC="/acpc/adm/etc"
COPYRIGHT=$'\xc2\xa9 2012-2023, ECPC System administration team'
cat << EOF
A contest configuration utility. The utility may run manually, or automatically with --no-interactive parameter
****** Do not run it during contest setup, contest.
Utility written by Egyptian CPC System team.
Copyright $(iconv -f utf-8 <<< ${COPYRIGHT})
For more information, please visit us at https://github.com/acpc-system


EOF
source checkers.sh
ERRMSG=""
ERRCODE=""
checkConfigFiles ${ERRMSG} ${ERRCODE}
[ ${ERRCODES} -ne 0 ] && genError "${ERRMSG}" ${ERRCODE}
echo "Done .. thank you .."



exit 0

