$ORIGIN .
$TTL 604800	; 1 week
#REVERSEDOMAINNAME#.in-addr.arpa	IN SOA	localhost. root.localhost. (
				6          ; serial
				604800     ; refresh (1 week)
				86400      ; retry (1 day)
				2419200    ; expire (4 weeks)
				604800     ; minimum (1 week)
				)
			NS	localhost.
$ORIGIN #REVERSEDOMAINNAME#.in-addr.arpa.
$TTL 602400	; 3 days 12 hours
